{
  "name": "dom-serializer",
  "version": "2.0.0",
  "description": "render domhandler DOM nodes to a string",
  "author": {
    "name": "Felix Boehm",
    "email": "me@feedic.com"
  },
  "sideEffects": false,
  "keywords": [
    "html",
    "xml",
    "render"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/cheeriojs/dom-serializer.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/esm/index.js",
  "exports": {
    "require": "./lib/index.js",
    "import": "./lib/esm/index.js"
  },
  "dependencies": {
    "domelementtype": "^2.3.0",
    "domhandler": "^5.0.2",
    "entities": "^4.2.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
